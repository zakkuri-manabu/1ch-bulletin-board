<!DOCTYPE html>
<html>
  <head>
  <%- include('./head_config') %>
  </head>
  <body>
    <%- include('./header') %>
    <div class="row">

      <div class="col s3">
        <%- include('./sidebar') %>
      </div>

      <div class="col s9">
        <% if(data) { %>
          <% for(var d of data) { %>
            <div class="card blue-grey darken-1">
              <div class="card-content white-text">
                <span class="card-title"><%= d._id %></span>
                <p><%= d.text %></p>
              </div>
              <div class="card-action">
                <a href='<%= d.url %>'>Message detail</a>
              </div>
            </div>
          <% } %>
        <% } else { %>
          <p>Message is null</p>
        <% } %>
      </div>

    </div>
    <%- include('./footer') %>
    <%- include('./body_config') %>
  </body>
</html>
